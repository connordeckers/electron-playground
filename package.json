{
  "name": "electron-playground",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main-process.js",
  "scripts": {
    "build": "webpack --env NODE_ENV=development --env target=render && webpack --env NODE_ENV=development --env target=main",
    "build-prod": "webpack --env NODE_ENV=production --env target=render && webpack --env NODE_ENV=production --env target=main",
    "server": "webpack serve --env NODE_ENV=development --env target=render --hot ",
    "launch": "webpack --env NODE_ENV=development --env target=main && electron .",
    "start": "npm run server & npm run launch",
    "pack": "rimraf ./pack && npm run build-prod && electron-packager . --out ./pack --overwrite"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
